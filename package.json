{
  "name": "url-selector",
  "version": "1.0.0",
  "description": "Web application where given a company name, it displays the domain associated to that company. The application is API driven, and it selects the most accurate company domain.",
  "main": "server.js",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec tools/testSetup.js \"src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Darin Boesch",
  "license": "MIT",
  "dependencies": {
    "async": "^2.5.0",
    "axios": "^0.15.3",
    "babel-polyfill": "6.8.0",
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "colors": "^1.1.2",
    "express": "^4.14.0",
    "mongoose": "^4.7.5",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.6",
    "react-router": "^3.0.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "request": "^2.82.0",
    "rimraf": "^2.6.2",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.1.11",
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.3",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^21.2.1",
    "extract-text-webpack-plugin": "^0.8.1",
    "jsdom": "^8.5.0",
    "mocha": "^4.0.1",
    "open": "0.0.5",
    "postcss-loader": "^0.4.3",
    "redux-immutable": "^4.0.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.10",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^1.9.0",
    "webpack-hot-middleware": "^2.19.1"
  }
}
